{
    "name": "watch-anything-together",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"pnpm backend:dev\" \"pnpm extension:browser:dev\" \"nodemon\" --kill-others",
        "backend:dev": "cd backend && pnpm dev",
        "extension:build:dev": "cd extension && pnpm build:dev",
        "extension:browser:dev": "cd extension && web-ext run --verbose --devtools"
    },
    "dependencies": {
        "nanoid": "4.0.2"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.60.1",
        "concurrently": "8.2.0",
        "eslint": "8.43.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "nodemon": "2.0.22",
        "prettier": "2.8.8",
        "typescript": "5.1.5",
        "web-ext": "7.6.2"
    }
}
