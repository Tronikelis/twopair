{
    "name": "backend",
    "type": "module",
    "scripts": {
        "build": "tsc && tsc-alias -p ./tsconfig.json",
        "build:check": "tsc --noEmit",
        "dev": "tsx --tsconfig ./tsconfig.json ./src/index.ts",
        "prod": "node ./dist/index.js"
    },
    "dependencies": {
        "pino": "8.14.1",
        "socket.io": "4.7.1"
    },
    "optionalDependencies": {
        "bufferutil": "4.0.7",
        "utf-8-validate": "5.0.10"
    },
    "devDependencies": {
        "tsc-alias": "1.8.6",
        "tsx": "3.12.7"
    }
}
